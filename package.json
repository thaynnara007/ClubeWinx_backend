{
  "name": "clubewinx_backend",
  "version": "1.0.0",
  "description": "backend da aplicacao",
  "main": "index.js",
  "scripts": {
    "start": "cd src && nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "sequelize db:migrate",
    "migrate:generate": "sequelize migration:generate",
    "migrate:undo": "sequelize db:migrate:undo",
    "seed": "sequelize db:seed:all",
    "seed:generate": "sequelize seed:generate",
    "prettier": "prettier --write \"**/*.js\"",
    "lint": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thaynnara007/ClubeWinx_backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thaynnara007/ClubeWinx_backend/issues"
  },
  "homepage": "https://github.com/thaynnara007/ClubeWinx_backend#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "sequelize": "^5.21.13"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "sequelize-cli": "^5.5.1"
  }
}
