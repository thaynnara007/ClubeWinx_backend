{
  "name": "clubewinx_backend",
  "version": "1.0.0",
  "description": "backend da aplicacao",
  "main": "index.js",
  "scripts": {
    "start": "cd src && nodemon server.js",
    "test": "jest --detectOpenHandles",
    "migrate": "sequelize db:migrate",
    "migrate:generate": "sequelize migration:generate",
    "migrate:undo": "sequelize db:migrate:undo",
    "seed": "sequelize db:seed:all",
    "seed:generate": "sequelize seed:generate",
    "prettier": "prettier --write \"**/*.js\"",
    "lint": "eslint . --fix",
    "swagger-autogen": "cd src/swagger && node swagger.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thaynnara007/ClubeWinx_backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thaynnara007/ClubeWinx_backend/issues"
  },
  "homepage": "https://github.com/thaynnara007/ClubeWinx_backend#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "@google-cloud/storage": "^5.8.3",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.6.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.5.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.13",
    "swagger-autogen": "^2.7.1",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "sequelize-cli": "^5.5.1",
    "supertest": "^6.1.3",
    "jest": "^26.6.3"
  }
}
